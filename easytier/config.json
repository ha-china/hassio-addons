{
  "name": "EasyTier",
  "version": "v2.4.5.2",
  "slug": "easytier",
  "description": "[老王杂谈说]：EasyTier 优秀的开源组网工具",
  "url": "https://github.com/ha-china/hassio-addons",
  "init": false,
  "arch": [
    "aarch64",
    "amd64"
  ],
  "privileged": [
    "NET_ADMIN"
  ],
  "environment": {
    "PGID": "0",
    "PUID": "0"
  },
  "devices": [
    "/dev/net/tun"
  ],
  "panel_icon": "mdi:application-variable",
  "boot": "manual",
  "map": [
    "config:rw"
  ],
  "host_network": true,
  "image": "ghcr.hasscn.top/desmond-dong/easytier/easytier-{arch}",
  "options": {
    "env_vars": []
  },
  "schema": {
    "env_vars": [
      {
        "name": "str?",
        "value": "str?",
        "type": "list(string|boolean|integer)?"
      }
    ]
  }
}